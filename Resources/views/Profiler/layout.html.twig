{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar%}
    {% set data = collector.data %}

    {% set icon %}
        <span>
            <img alt="Pomm" src="data:image/png;base64," />
            <span class="sf-toolbar-status{% if 50 < data.total_count %} sf-toolbar-status-yellow{% endif %}">
                {{ data.total_count }}
            </span>
            {% if data.total_count > 0 %}
                <span class="sf-toolbar-info-piece-additional-detail">in {{ '%0.2f'|format(data.total_time) }} ms</span>
            {% endif %}
        </span>
    {% endset %}

    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Queries</b>
            <span>{{ data.query_count }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Query time</b>
            <span>{{ '%0.2f'|format(data.query_time) }} ms</span>
        </div>

        <div class="sf-toolbar-info-piece">
            <b>Performs</b>
            <span>{{ data.perform_count }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Perform time</b>
            <span>{{ '%0.2f'|format(data.perform_time) }} ms</span>
        </div>

        <div class="sf-toolbar-info-piece">
            <b>Execute prepared</b>
            <span>{{ data.execute_count }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Execute time</b>
            <span>{{ '%0.2f'|format(data.execute_time) }} ms</span>
        </div>

        <div class="sf-toolbar-info-piece">
            <b>Prepare</b>
            <span>{{ data.prepare_count }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Transations</b>
            <span>{{ data.transaction_count }}</span>
        </div>

        <div class="sf-toolbar-info-piece">
            <b>Total queries</b>
            <span>{{ data.total_count }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Total time</b>
            <span>{{ '%0.2f'|format(data.total_time) }} ms</span>
        </div>
    {% endset %}

    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    {% set data = collector.data %}
    <span class="label">
        <span class="icon"><img alt="Goat" src="data:image/png;base64," /></span>
        <strong>Goat</strong>
        <span class="count">
            <span>{{ data.total_count }}</span>
            <span>{{ '%0.0f'|format(data.total_time) }} ms</span>
        </span>
    </span>
{% endblock %}
